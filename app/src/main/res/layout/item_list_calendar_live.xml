<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="horizontal"
    android:layout_width="match_parent"
    android:layout_height="70dp"
    tools:ignore="missingPrefix"
    android:layout_marginLeft="10dp"
    android:layout_marginRight="10dp"
    android:layout_marginBottom="5dp"
    android:layout_marginTop="5dp"
    android:background="@drawable/calendar_list_back_live">
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="15dp">
            <TextView
                android:id="@+id/startTime2"
                android:layout_width="80dp"
                android:layout_height="wrap_content"

                android:text="2:00 PM"
                android:fontFamily="@font/nanumsquaure_r"
                android:textSize="12dp"
                android:layout_marginBottom="3dp"
                android:textColor="#ffffff"/>

            <TextView
                android:id="@+id/endTime2"
                android:layout_width="80dp"
                android:layout_height="wrap_content"

                android:text="3:00 PM"
                android:fontFamily="@font/nanumsquaure_r"
                android:textSize="12dp"
                android:textColor="#ffffff"/>
        </LinearLayout>
    <TextView
        android:id="@+id/problem_name2"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="17dp"

        android:text="교육청 1"
        android:fontFamily="@font/nanumsquaure_b"
        android:textSize="20dp"
        android:textColor="#ffffff"/>
    <ImageView
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:background="@drawable/home_top_left"
        android:layout_marginTop="19dp"/>
</LinearLayout>